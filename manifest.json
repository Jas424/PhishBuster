{
    "manifest_version": 3,
    "name": "PhishBuster",
    "version": "1.0",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }],
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab","scripting"]
  }
  